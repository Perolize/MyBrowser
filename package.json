{
  "name": "mybrowser",
  "version": "0.0.5",
  "description": "A fully customizable web browser that really was made for you",
  "main": "./dist/main.js",
  "scripts": {
    "build:watch": "tsc -p src/ -w",
    "start": "npm run movesssets && concurrently \"npm run build:watch\" \"electron .\" \"npm run watchassets\" \"npm run watchassets2\" ",
    "movesssets": "cpx \"src/*.{html,css}\" dist && cpx \"src/**/*.{html,css}\" dist &&  cpx \"src/**/*.{js,js.map}\" dist",
    "watchassets": "onchange \"src/**/*.css\" \"src/**/*.html\" -e \"dist/*\" -v -- cpx \"src/**/*.{html,css}\" dist",
    "watchassets2": "onchange \"src/*.css\" \"src/*.html\" -e \"dist/*\" -v -- cpx \"src/*.{html,css}\" dist"
  },
  "build": {
    "appId": "",
    "mac": {
      "category": ""
    },
    "protocols": [
      {
        "name": "mybrowser",
        "role": "Viewer",
        "schemes": [
          "mybrowser"
        ]
      }
    ]
  },
  "repository": "",
  "keywords": [
    "MyBrowser",
    "Browser",
    "Custom",
    "Developer",
    "Shopping"
  ],
  "author": "Myputer Inc.",
  "license": "MPL 2.0",
  "productName": "MyBrowser",
  "electronVersion": "1.4.12",
  "devDependencies": {
    "@types/jquery": "^2.0.41",
    "concurrently": "^3.1.0",
    "electron-connect": "^0.6.1",
    "electron-packager": "^8.6.0",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-tslint": "^7.1.0",
    "gulp-typescript": "^3.1.4",
    "onchange": "^3.2.1",
    "run-sequence": "^1.2.2",
    "tslint": "^4.5.1",
    "typescript": "~2.0.10"
  },
  "dependencies": {
    "del": "^2.2.2",
    "dragula": "^3.7.2",
    "electron-context-menu": "^0.8.0",
    "is-url": "^1.2.2",
    "jquery": "^3.1.1",
    "normalize-url": "^1.9.1",
    "normalize.css": "^5.0.0"
  }
}
